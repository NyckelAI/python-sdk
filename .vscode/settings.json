{
    "python.formatting.provider": "none",
    "python.formatting.blackArgs": [
        "--line-length",
        "120"
    ],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "cSpell.words": [
        "chunkify",
        "Gruezi",
        "Hola",
        "nyckel",
        "pytest",
        "tqdm"
    ],
    "mypy.targets": [
        "src"
    ]
}